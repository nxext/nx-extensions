{
  "projectType": "application",
  "sourceRoot": "e2e/ionic-react-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn nx build ionic-react"
          },
          {
            "command": "yarn nx run-e2e-tests ionic-react-e2e"
          }
        ],
        "parallel": false,
        "color": true
      }
    },
    "run-e2e-tests": {
      "executor": "@nrwl/nx-plugin:e2e",
      "options": {
        "target": "ionic-react:build",
        "jestConfig": "e2e/ionic-react-e2e/jest.config.ts"
      }
    },
    "e2e-verdaccio": {
      "executor": "@nxext/e2e:e2e",
      "options": {
        "jestConfig": "e2e/ionic-react-e2e/jest.config.ts"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["ionic-react"]
}
