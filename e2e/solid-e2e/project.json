{
  "projectType": "application",
  "root": "e2e/solid-e2e",
  "sourceRoot": "e2e/solid-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn nx build solid"
          },
          {
            "command": "yarn nx run-e2e-tests solid-e2e"
          }
        ],
        "parallel": false,
        "color": true
      }
    },
    "run-e2e-tests": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "e2e/solid-e2e/jest.config.ts",
        "passWithNoTests": true,
        "runInBand": true
      },
      "outputs": ["coverage/e2e/solid"]
    },
    "e2e-verdaccio": {
      "executor": "@nxext/e2e:e2e",
      "options": {
        "jestConfig": "e2e/solid-e2e/jest.config.ts"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["solid", "vite"]
}
