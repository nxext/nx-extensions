    "@angular/core": "13.3.2",
    "clsx": "^1.1.1",
    "recrawl-sync": "^2.2.1",
    "source-map": "^0.7.3",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@angular/compiler": "13.3.2",
    "@nrwl/angular": "14.1.4",
    "@nrwl/cli": "14.1.4",
    "@nrwl/cypress": "14.1.4",
    "@nrwl/devkit": "14.1.4",
    "@nrwl/eslint-plugin-nx": "14.1.4",
    "@nrwl/jest": "14.1.4",
    "@nrwl/js": "14.1.4",
    "@nrwl/linter": "14.1.4",
    "@nrwl/node": "14.1.4",
    "@nrwl/nx-cloud": "14.0.3",
    "@nrwl/nx-plugin": "14.1.4",
    "@nrwl/react": "14.1.4",
    "@nrwl/storybook": "14.1.4",
    "@nrwl/workspace": "14.1.4",
    "@preact/preset-vite": "^2.1.7",
    "@stencil/core": "2.15.1",
    "@sveltejs/kit": "^1.0.0-next.301",
    "@swc/cli": "~0.1.57",
    "@swc/core": "^1.2.168",
    "@swc/helpers": "~0.3.8",
    "@swc/jest": "0.2.15",
    "@types/globrex": "^0.1.2",
    "@types/is-ci": "^3.0.0",
    "@types/jest": "27.4.1",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "all-contributors-cli": "^6.20.0",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.3.0",
    "dotenv": "16.0.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-svelte3": "^3.4.1",
    "fs-extra": "^10.0.1",
    "globrex": "^0.1.2",
    "husky": "^6.0.0",
    "ignore": "^5.2.0",
    "is-ci": "^3.0.1",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "nx": "14.1.4",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3",
    "rxjs-for-await": "^1.0.0",
    "swc-ast-helpers": "^0.0.8-beta.1",
    "ts-jest": "27.1.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.1.0",
