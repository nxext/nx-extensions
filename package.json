  "peerDependencies": {
    "rxjs": "^7.4.0"
  },
  "resolutions": {
    "terser": "^4.0.0",
    "fast-glob": "3.2.7"
  },
  "lint-staged": {
    "nx.json": [
      "yarn format --uncommitted"
    ],
    "*.{js,json,css,scss,md,ts,html,graphql}": [
      "yarn format --uncommitted"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
