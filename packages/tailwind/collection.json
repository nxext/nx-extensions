{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "name": "tailwind",
  "version": "0.0.1",
  "schematics": {
    "stencil": {
      "factory": "./src/schematics/stencil/schematic#tailwindStencilSchematic",
      "schema": "./src/schematics/stencil/schema.json",
      "description": "stencil tailwind schematic"
    },
    "svelte": {
      "factory": "./src/schematics/svelte/generator#tailwindSvelteSchematic",
      "schema": "./src/schematics/svelte/schema.json",
      "description": "svelte schematic"
    }
  },
  "generators": {
    "stencil": {
      "factory": "./src/schematics/stencil/schematic#tailwindStencilGenerator",
      "schema": "./src/schematics/stencil/schema.json",
      "description": "stencil tailwind schematic"
    },
    "svelte": {
      "factory": "./src/schematics/svelte/generator#tailwindSvelteGenerator",
      "schema": "./src/schematics/svelte/schema.json",
      "description": "svelte schematic"
    }
  }
}
