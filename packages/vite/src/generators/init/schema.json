{
  "$schema": "http://json-schema.org/schema",
  "$id": "LibertyDevViteInit",
  "title": "Init Vite Plugin",
  "type": "object",
  "properties": {
    "unitTestRunner": {
      "description": "Adds the specified unit test runner",
      "type": "string",
      "enum": [
        "vitest",
        "jest",
        "none"
      ],
      "default": "vitest"
    },
    "skipFormat": {
      "description": "Skip formatting files",
      "type": "boolean",
      "default": false
    }
  },
  "required": []
}
