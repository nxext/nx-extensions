{
  "$schema": "http://json-schema.org/schema",
  "name": "vitest",
  "version": "0.0.1",
  "generators": {
    "init": {
      "factory": "./src/generators/init/init",
      "schema": "./src/generators/init/schema.json",
      "description": "init generator"
    },
    "vitest-project": {
      "factory": "./src/generators/vitest-project/vitest-project",
      "schema": "./src/generators/vitest-project/schema.json",
      "description": "vitest-project generator"
    }
  },
  "schematics": {
    "init": {
      "factory": "./src/generators/init/init#vitestInitSchematic",
      "schema": "./src/generators/init/schema.json",
      "description": "init generator"
    },
    "vitest-project": {
      "factory": "./src/generators/vitest-project/vitest-project#vitestProjectSchematic",
      "schema": "./src/generators/vitest-project/schema.json",
      "description": "vitest-project generator"
    }
  }
}
