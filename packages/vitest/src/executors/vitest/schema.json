{
  "version": 2,
  "outputCapture": "direct-nodejs",
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "title": "Vitest executor",
  "description": "",
  "type": "object",
  "properties": {
    "passWithNoTests": {
      "type": "boolean",
      "description": "Pass the build if no tests are found",
      "default": false
    },
    "testNamePattern": {
      "type": "string",
      "description": "Pattern to match test names"
    },
    "vitestMode": {
      "type": "string",
      "enum": ["test", "benchmark"],
      "default": "test"
    }
  },
  "required": []
}
