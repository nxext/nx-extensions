{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "VitestProject",
  "title": "",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "For which project do you want to initialize vitest?"
    },
    "framework": {
      "description": "Framework to use.",
      "type": "string",
      "enum": ["generic", "svelte", "react"],
      "default": "generic"
    }
  },
  "required": ["project"]
}
