import { describe, it, expect } from 'vitest'

import { mount } from '@vue/test-utils'
import <%= className %> from './<%= className %>.vue'

describe('<%= className %>', () => {
  it('renders properly', () => {
    const wrapper = mount(<%= className %>, { props: { msg: 'Hello Vitest' } })
    expect(wrapper.text()).toContain('Hello Vitest')
  })
})
